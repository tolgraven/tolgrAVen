{
  "dependencies": {
    "react-syntax-highlighter": "^15.5.0",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "autoprefixer": "^10.2.5",
    "firebase": "^11.8.0",
    "leaflet": "1.9.4",
    "sass": "^1.92.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.3.0",
    "postcss-cli": "9.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "4.2.0",
    "shadow-cljs": "^3.1.7"
  },
  "scripts": {
    "sassc": "lein sassc once",
    "sass": "mkdir -p resources/public/css/tolgraven && node-sass --output-style compressed --no-source-maps resources/scss/main.scss > resources/public/css/tolgraven/main.css",
    "sass:dev": "sass --watch resources/scss/main.scss:resources/public/css/tolgraven/main.css --embed-source-map --embed-sources",
    "sass:build": "sass resources/scss/main.scss:resources/public/css/tolgraven/main.css --style=compressed --no-error-css",
    "postcss": "postcss resources/public/css/tolgraven/main.css -o resources/public/css/tolgraven/main.min.css -u autoprefixer --no-map",
    "build": "npm run sass:build && npm run postcss"
  },
  "devDependencies": {
    "shadow-cljs": "^3.1.7"
  }
}
